<%- include('../partials/header') %>
<%- include('../partials/breadcrumb') %>
<li class="breadcrumb-item"><a href="/skills">All Skills</a></li>
<li class="breadcrumb-item"><a href="/skills/<%= skill.id %>">Skill Details</a></li>
<li class="breadcrumb-item active" aria-current="page"><%= title %></li>
<%- include('../partials/breadcrumbEnd') %>

<form action="/skills/<%= skill.id %>?_method=PUT" method="POST" autocomplete="off">
    <input type="text" name="name" value="<%= skill.name %>"><br>
    <br>
    <button type="submit" class="btn btn-light">Update Skill</button>
</form>
<%- include('../partials/footer') %>